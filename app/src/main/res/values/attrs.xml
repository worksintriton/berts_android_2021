<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MenuOptions">
        <attr name="titleTab1" format="string" localization="suggested" />
        <attr name="titleTab2" format="string" localization="suggested" />
        <attr name="titleTab3" format="string" localization="suggested" />
        <attr name="iconMenu1" format="integer" />
        <attr name="iconMenuOn1" format="integer" />
        <attr name="iconMenu2" format="integer" />
        <attr name="iconMenu3" format="integer" />
        <attr name="titleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="hint" format="string" localization="suggested" />
        <attr name="inputType" format="integer" localization="suggested" >
            <enum name="email" value="0"/>
            <enum name="password" value="1"/>
            <enum name="text_normal" value="2"/>
            <enum name="number" value="3"/>
            <enum name="none" value="4"/>
        </attr>

    </declare-styleable>

    <declare-styleable name="CustomTextarea">
        <attr name="hinText" format="string" localization="suggested" />
        <attr name="input" format="integer" localization="suggested" >
            <enum name="email" value="0"/>
            <enum name="password" value="1"/>
            <enum name="text_normal" value="2"/>
            <enum name="number" value="3"/>
        </attr>

    </declare-styleable>

    <declare-styleable name="CustomButtonChoose">
        <attr name="title" format="string" localization="suggested" />
        <attr name="value" format="string" localization="suggested" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
        <attr name="fill_color" format="color"/>
    </declare-styleable>

</resources>